{
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/assets/.*\\.js$",
      "headers": {
        "Content-Type": "text/javascript"
      }
    },
    {
      "src": "/assets/.*\\.css$",
      "headers": {
        "Content-Type": "text/css"
      }
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}